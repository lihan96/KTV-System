<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.edu.sysu.Dao.ManagerDao">

    <select id="getManager" resultType="Manager" parameterType="string">
        SELECT *
        FROM tb_manager
        WHERE username = #{username}
    </select>

</mapper>